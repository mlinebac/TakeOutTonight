    #form.container-fluid
      .row
        .center
          .col-md-6.col-sm-6.col-xs-12
            form(method='post', action = "", id = "newForm")
              .form-group
                label.control-label(for='name')
                  | Name
                input#name.form-control(name='name', type='text')
              .form-group
                label.control-label.requiredField(for='email')
                  | Email
                  span.asteriskField
                    | *
                input#email.form-control(name='email', type='text')
              .form-group
                label.control-label(for='select')
                  | Select a Choice
                select#select.select.form-control(name='select')
                  option(value='First Choice')
                    | First Choice
                  option(value='Second Choice')
                    | Second Choice
                  option(value='Third Choice')
                    | Third Choice
              .form-group
                label.control-label
                  | Select a Choice
                div#radio
                  .radio
                    label.radio
                      input(name='radio', type='radio', value='First Choice')
                      |          First Choice
                  .radio
                    label.radio
                      input(name='radio', type='radio', value='Second Choice')
                      |          Second Choice
                  .radio
                    label.radio
                      input(name='radio', type='radio', value='Third Choice')
                      |          Third Choice
              .form-group
                label.control-label(for='message')
                  | Message
                textarea#message.form-control(cols='40', name='message', rows='10')
              .form-group
                div
                  button.btn.btn-primary(id = 'submitForm', name='submitForm', type='submit')
                    | Submit

    #mail.container-fluid
      .row
        .col-md-12
          a.btn.btn-info(href= 'mailto:aliamtodd@gmail.com', role= 'button') Send Message
            span.glyphicon.glyphicon-envelope(aria-hidden='true')
      hr
